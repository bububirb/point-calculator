config/icon="res://assets/icons/platform_icons/platform_icon_128px.svg"
config/windows_native_icon="res://assets/icons/platform_icons/windows_icon.ico"

[autoload]

Globals="*res://scripts/globals.gd"
Tally="*res://scripts/tally.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/energy_saving/keep_screen_on=false
window/per_pixel_transparency/allowed=true
